{
  "name": "node-starter-kit",
  "version": "0.5.0",
  "description": "A simple node starter kit ",
  "repository": {
    "type": "git",
    "url": "git://github.com/pebblecode/node-starter-kit.git"
  },
  "main": "src/index.js",
  "scripts": {
    "compile": "babel --source-maps --out-dir lib/ src/",
    "lint": "eslint --ext .js --ext .jsx ./ && echo No linting errors.",
    "test": "node_modules/.bin/mocha test/ --recursive --compilers js:babel-core/register",
    "test:watch": "npm run test -- --watch",
    "coverage": "istanbul cover _mocha -- --compilers js:babel-core/register --colors --reporter dot test/",
    "prepublish": "npm run compile"
  },
  "keywords": [],
  "author": {
    "name": "PebbleCode",
    "email": "opensource@pebblecode.com",
    "url": "http://pebblecode.com"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.x.x",
    "babel-core": "^6.1.4",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.4",
    "babel-preset-react": "^6.1.4",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "eslint-config-pebblecode": "^1.0.0",
    "istanbul": "^0.4.0",
    "mocha": "^2.2.1",
    "pre-commit": "^1.0.6"
  },
  "engines": {
    "node": "4.x.x",
    "npm": "2.x.x"
  }
}
